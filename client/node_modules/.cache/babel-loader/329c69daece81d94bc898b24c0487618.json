CA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA,CAAA,KAAA;AACA,QAAA,KAAA,CAAA,gBAAA,GAAA,KAAA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,KAAA,EAAA;AACA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,KAAA;AACA,OAdA,EAeA,IAfA,CAeA,YAAA,CACA;AACA,OAjBA;AAkBA,KArBA;AAsBA,IAAA,aAtBA,yBAsBA,IAtBA,EAsBA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,IAAA,EAAA,QAAA;AACA,KAxBA,CAyBA;AACA;AACA;AACA;AACA;AACA;AACA;;AA/BA;AArBA,CAAA","sourcesContent":["<template>\r\n  <v-container>\r\n    <v-text-field\r\n      label=\"Buscar Articulo de MercadoLibre\"\r\n      placeholder=\"Ej. Descarrilador Shimano\"\r\n      filled\r\n      v-model=\"busquedaInput\"\r\n      @keydown.enter=\"buscarML\"\r\n    >\r\n      <v-icon slot=\"append\">\r\n        mdi-shopping-search\r\n      </v-icon>\r\n    </v-text-field>\r\n    <span>Mostrar articulo con:</span>\r\n    <v-chip>Mas ventas </v-chip>\r\n    <v-chip>asdasd</v-chip>\r\n    <v-chip>asdasd</v-chip>\r\n    <v-data-table\r\n      :loading=\"loadingMainTabla\"\r\n      loading-text=\"Cargando... Espere\"\r\n      multi-sort\r\n      :headers=\"headers\"\r\n      :items=\"items\"\r\n      :items-per-page=\"10\"\r\n      class=\"elevation-1\"\r\n    >\r\n      <template v-slot:item.price=\"{ item }\">\r\n        $ {{ item.price}}\r\n      </template>\r\n      <template v-slot:item.links=\"{ item }\">\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n              @click=\"abrirPestania(item.permalink)\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            >\r\n              mdi-link-variant\r\n            </v-icon>\r\n          </template>\r\n          <span>Ir al articulo</span>\r\n        </v-tooltip>\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n              @click=\"abrirPestania(item.thumbnail)\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            >\r\n              mdi-image\r\n            </v-icon>\r\n          </template>\r\n          <span>Ver imagen</span>\r\n        </v-tooltip>\r\n        <v-tooltip bottom>\r\n          <template v-slot:activator=\"{ on, attrs }\">\r\n            <v-icon\r\n              @click=\"abrirPestania(item.seller.permalink)\"\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n            >\r\n              mdi-account-circle\r\n            </v-icon>\r\n          </template>\r\n          <span>Pagina del vendedor</span>\r\n        </v-tooltip>\r\n\r\n      </template>\r\n      <!-- <template v-slot:item.seller.permalink=\"{ item }\">\r\n        <a :href=\"item.seller.permalink\">\r\n          {{ item.seller.pernalink }}\r\n        </a>\r\n      </template> -->\r\n    </v-data-table>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data () {\r\n    return {\r\n      busquedaInput: \"\",\r\n      headers: [\r\n        {\r\n          text: \"ID MLA\",\r\n          align: \"start\",\r\n          sortable: false,\r\n          value: \"id\",\r\n        },\r\n        { text: \"Articulo\", value: \"title\" },\r\n        { text: \"Precio\", value: \"price\" },\r\n        { text: \"Links\", value: \"links\", sortable: false },\r\n      ],\r\n      items: [],\r\n      loadingMainTabla: false,\r\n    };\r\n  },\r\n  created () { },\r\n  mounted () { },\r\n  methods: {\r\n    buscarML () {\r\n      this.loadingMainTabla = true;\r\n      this.$axios\r\n        .get(\r\n          \"https://api.mercadolibre.com/sites/MLA/search?q=\" +\r\n          encodeURIComponent(this.busquedaInput)\r\n        )\r\n        .then((response) => {\r\n          // handle success\r\n          this.items = response.data.results;\r\n          console.log(this.items);\r\n          this.loadingMainTabla = false;\r\n        })\r\n        .catch((error) => {\r\n          // handle error\r\n          console.log(error);\r\n        })\r\n        .then(() => {\r\n          // always executed\r\n        });\r\n    },\r\n    abrirPestania (link) {\r\n      window.open(link, \"_blank\");\r\n    },\r\n    // calcular () {\r\n    //   this.items.forEach(element => {\r\n    //     console.log(element.price)\r\n    //   })\r\n    //   this.items.sort((a, b) => (a.price < b.price) ? 1 : -1)\r\n    //   console.log(this.items)\r\n    // }\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":"src/components"}]}